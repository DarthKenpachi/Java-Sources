import javax.swing.*;


public class MainGui 
{

	public static void main(String[] args)
	
	{
		
		GuiPanel panel = new GuiPanel();
		JFrame frame = new JFrame();
		
		frame.add(panel);
		frame.setSize(800,800);
		frame.setVisible(true);
		frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
	}
	
}
